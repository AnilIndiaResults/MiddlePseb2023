                return false;
            }
            if (tdp.value == "0") {
                alert("Disability Percentage should be between 1-100");
                document.getElementById("DP").focus();
                return false;
            }
            if (wri.value == "" || wri.value == null) {
                alert("Please Select Scribe Writter");
                document.getElementById("scribeWriter").focus();
                return false;
            }

            if (tdp.value > 100) {
                alert("Disability Percentage should be smaller than 100");
                document.getElementById("DP").focus();
                return false;
            }

        }

