        //------------------------------Start multiple Selection--------------------
        if (subS1.value != "" && subS2.value != "" && subS3.value != "" && subS4.value != "" && subS5.value) {
            //alert("Hello");

            // The number of dropdowns you have (use the naming convention 'dropx' as an id attribute)
            var iDropdowns = 6;
            var sValue;
            var sValue2;
            // Loop over dropdowns
            for (var i = 2; i <= iDropdowns; ++i) {
                // Get selected value
                sValue = document.getElementById('subS' + i).value;
                // Nested loop - loop over dropdowns again
