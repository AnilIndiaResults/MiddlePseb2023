@model PsebJunior.Models.ReportModel

<style type="text/css">
    td {
        font-size: 11px;
        padding: 1px;
        vertical-align: top
    }
</style>
@*<link rel="stylesheet" href="https://registration.pseb.ac.in/Content/Print-4P.css" />*@
@*<h1 class="noprint">Form Wise Report</h1>*@

@if (ViewBag.TotalCount > 0)
{
<div class="col-md-12 text-right noprint">
    @if (ViewBag.TotalCount > 0)
    {
        <br />
        <input type="button" value="Download Report" onclick="javascript:document.location.href='@Url.Action("ExportDataFromDataTable", "Report", new { dt = @TempData["PrimaryMiddleSummaryReportsData"], FileNAME = @TempData["FileNAME"].ToString() })'" />
    }   
</div>
    <div class="clearfix"></div>

    <table style="border-collapse:collapse;width:100%;font-family:Arial, Helvetica, sans-serif;font-size:12px" border="1">
        <tr>
            <td colspan="2" style="border-right:0 !important">
                <div style="padding:2px"><img src="~/Images/Printlogo.jpg" width="60px" height="60px" alt="PSEB" /></div>
            </td>
            <td colspan="5" style="font-size:14px;text-align:center;line-height:22px;vertical-align:top;border-left:0 !important;border-right:0 !important;padding-top:8px;">
                <span style="font-size:28px">Punjab School Education Board</span>
                <br />
                <span>  Primary-Middle Pending Schools For Registration, Session (@PsebPrimaryMiddle.Models.MasterClass.SessionFullYear)</span>
            </td>
            <td colspan="2" style="border-left:0 !important" align="right">Print Date : @DateTime.Now.ToString("dd/MM/yyyy hh:mm tt") &nbsp;</td>
        </tr>

        <tr>
            <td align="center">distnm</td>
            <td align="center">udisecode</td>
            <td align="center">Schl</td>
            <td align="center">schlnme</td>
            <td align="center">usertype</td>
            <td align="center">mobile</td>
            <td align="center">PHONE</td>
            <td align="center">emailid</td>
            <td align="center">status</td>
        </tr>
        @foreach (System.Data.DataRow dr in Model.StoreAllData.Tables[0].Rows)
        {
            <tr>
                <td align="center">@dr["distnm"].ToString()</td>
                <td align="center">@dr["udisecode"].ToString()</td>
                <td align="center">@dr["Schl"].ToString()</td>
                <td align="center">@dr["schlnme"].ToString()</td>
                <td align="center">@dr["usertype"].ToString()</td>
                <td align="center">@dr["mobile"].ToString()</td>
                <td align="center">@dr["PHONE"].ToString()</td>
                <td align="center">@dr["emailid"].ToString()</td>
                <td align="center">@dr["status"].ToString()</td>
            </tr>
        }
    </table>
}




