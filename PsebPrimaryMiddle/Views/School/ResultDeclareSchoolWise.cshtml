<script type="text/javascript">
    function dee() {

        if ($("#SelFilter").val() == "") {
            $("#reqsession").slideDown("slow");
            return false;
        }
        if ($("#SelFilter").val() != "" && $("#SearchString").val() == "") {
            $("#reqsellist").slideDown("slow");
            return false;
        }


    }

</script>
