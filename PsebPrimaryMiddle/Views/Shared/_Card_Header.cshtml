<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title Punjab School Education Board</title>
    <noscript>
            <style type="text/css">
                #HPL {display:none}
            </style>
        <charset style="background-color:#cc0000;color:#ffd800;font-size:24px;text-align:center;padding:10px 0">Your browser does not support JavaScript!</charset>
    </noscript>    
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
        <div id="HPL">
    <div class="header_outer noprint">
      <div class="container">
<div class="row">
           <div class="col-xs-3 col-md-3"><img src="~/Images/pseb_logo.png" alt="Punjab School Education Board" class="img-responsive" /></div>
                <div class="col-xs-9 col-md-6"><center> <img src="~/Images/PSEB_Text.png" alt="Punjab School Education Board" class="img-responsive" /></center></div>
                <div class="col-xs-12 col-md-3 text-right">&nbsp;</div>
        </div></div>
    </div>

    <div class="print-width">
        @RenderBody()
    </div>
    <footer class="bs-docs-footer noprint">
        <div class="container text-center">
           All Rights Reserved at Punjab School Education Board, 1969 - 2019
        </div>
    </footer>

            </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/Content/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
